        "import sys\n",
        "sys.path.append('/content/drive/MyDrive/')\n",
        "from rule_miner import *\n",
        "\n",
        "support_t = 25;\n",
        "confidence_t = 0.85;\n",
        "\n",
        "ruleminer = RuleMiner(support_t, confidence_t);\n",
        "\n",
        "rules = ruleminer.get_association_rules(one_hot_encoded_df)\n",
        "print(rules)"
      ],
